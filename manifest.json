{
  "manifest_version": 3,
  "name": "Color Picker",
  "version": "2.0",
  "description": "A color picker extension that works on all browser versions.",
  "permissions": [
    "scripting",
    "activeTab",
    "storage"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "icon128.png"
    }
  },
  "icons": {
    "128": "icon128.png"
  }
}